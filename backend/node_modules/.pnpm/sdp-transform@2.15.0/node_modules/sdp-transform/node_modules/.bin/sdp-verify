#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sahil/Developer/Super30/unacedmy-clone/backend/node_modules/.pnpm/sdp-transform@2.15.0/node_modules/sdp-transform/node_modules:/Users/sahil/Developer/Super30/unacedmy-clone/backend/node_modules/.pnpm/sdp-transform@2.15.0/node_modules:/Users/sahil/Developer/Super30/unacedmy-clone/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sahil/Developer/Super30/unacedmy-clone/backend/node_modules/.pnpm/sdp-transform@2.15.0/node_modules/sdp-transform/node_modules:/Users/sahil/Developer/Super30/unacedmy-clone/backend/node_modules/.pnpm/sdp-transform@2.15.0/node_modules:/Users/sahil/Developer/Super30/unacedmy-clone/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../checker.js" "$@"
else
  exec node  "$basedir/../../checker.js" "$@"
fi
